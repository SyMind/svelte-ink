  "license": "MIT",
  "devDependencies": {
    "@types/cross-spawn": "^6.0.2",
    "@types/node": "^18.11.18",
    "@types/prompts": "^2.4.2",
    "@types/validate-npm-package-name": "^4.0.0",
    "chalk": "^5.2.0",
    "cpy": "^7.3.0",
    "cross-spawn": "^7.0.3",
    "prompts": "^2.4.2",
    "rimraf": "^4.1.1",
    "validate-npm-package-name": "^5.0.0"
  }
}
